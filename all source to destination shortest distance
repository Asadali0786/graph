void bfs(vector<vector<pair<int, int>>> &g)
{

    vector<int> dist(g.size(), INT_MAX);

    queue<int> q;
    q.push(0);
    dist[0] = 0;
    while (!q.empty())
    {

        int node = q.front();
        q.pop();

        for (auto &x : g[node])
        {
            if (dist[node] + x.ss < dist[x.ff])
            {
                dist[x.ff] = dist[node] + x.ss;
                q.push(x.ff);
            }
        }
    }
 
}
